{
    "name": "faysal0x1/recommendation-module",
    "description": "AI-ready recommendation engine module for Laravel storefronts.",
    "type": "laravel-module",
    "license": "MIT",
    "version": "1.0.1",
    "keywords": [
        "laravel",
        "module",
        "recommendation"
    ],
    "authors": [
        {
            "name": "Faysal",
            "homepage": "https://github.com/faysal0x1"
        }
    ],
    "require": {
        "php": "^8.2",
        "nwidart/laravel-modules": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Modules\\Recommendation\\": ""
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Modules\\Recommendation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "module-dir": "Modules"
    }
}
